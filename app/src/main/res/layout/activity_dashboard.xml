<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".DashboardActivity">

    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="?attr/actionModeCloseDrawable"
            android:id="@+id/activity_dashboard_back_button" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="14dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="12dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/activity_dashboard_title"
            android:textColor="@color/custom_navy_A"
            android:textStyle="bold"
            android:textSize="26sp"
            android:id="@+id/activity_dashboard_title"
            app:fontFamily="@font/spartan"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/activity_dashboard_back_button"
            app:layout_constraintBottom_toBottomOf="@+id/activity_dashboard_back_button"/>
    <ToggleButton
            android:text="ToggleButton"
            android:layout_width="187dp"
            android:layout_height="52dp" android:id="@+id/toggleButton" tools:layout_editor_absoluteY="230dp"
            app:layout_constraintEnd_toStartOf="@+id/toggleButton2"/>
    <ToggleButton
            android:text="ToggleButton"
            android:layout_width="173dp"
            android:layout_height="50dp" android:id="@+id/toggleButton2" tools:layout_editor_absoluteY="230dp"
            tools:layout_editor_absoluteX="219dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>