<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SurveyMiddle7Activity">

    <TextView
            android:layout_width="360dp"
            android:layout_height="62dp"
            android:text="@string/activity_survey_middle7_question"
            android:textColor="@color/custom_navy_A"
            android:textSize="24sp"
            android:textStyle="bold"
            android:id="@+id/activity_survey_middle7_question"
            android:gravity="center"
            app:fontFamily="@font/spartan"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.484"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="180dp"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_back_arrow"
            android:id="@+id/activity_survey_middle7_back_button" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="14dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="12dp" android:minHeight="48dp" tools:ignore="SpeakableTextPresentCheck"
            tools:srcCompat="@drawable/ic_back_arrow"/>
    <Button
            android:text="@string/activity_survey_next"
            android:layout_width="361dp"
            android:layout_height="62dp" android:id="@+id/activity_survey_middle7_next_button"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="9dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"/>
    <com.google.android.material.slider.Slider
            android:id="@+id/slide"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:value="0"
            android:stepSize="1"
            android:valueFrom="0"
            android:valueTo="10"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="24dp"
            app:layout_constraintVertical_bias="0.491"/>
    <TextView
            android:text="not enjoyable at all"
            android:layout_width="108dp"
            android:layout_height="52dp"
            android:id="@+id/textView"
            android:gravity="center"
            app:fontFamily="@font/spartan"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="288dp"
            app:layout_constraintVertical_bias="0.547"/>
    <TextView
            android:text="very enjoyable"
            android:gravity="center"
            android:layout_width="83dp"
            android:layout_height="58dp"
            android:id="@+id/textView2"
            app:fontFamily="@font/spartan"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="10dp"
            app:layout_constraintVertical_bias="0.54"/>


</androidx.constraintlayout.widget.ConstraintLayout>