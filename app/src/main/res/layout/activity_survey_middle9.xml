<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SurveyMiddle9Activity">

    <TextView
            android:layout_width="360dp"
            android:layout_height="62dp"
            android:text="@string/activity_survey_middle9_question"
            android:textColor="@color/custom_navy_A"
            android:textSize="20sp"
            android:id="@+id/activity_survey_middle9_question"
            app:fontFamily="@font/spartan" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.484"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="84dp"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_back_arrow"
            android:id="@+id/activity_survey_middle9_back_button" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="14dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="12dp" android:minHeight="48dp" tools:ignore="SpeakableTextPresentCheck"
            tools:srcCompat="@drawable/ic_back_arrow"/>
    <Button
            android:text="@string/activity_survey_next"
            android:layout_width="361dp"
            android:layout_height="62dp" android:id="@+id/activity_survey_middle9_next_button"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="9dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>